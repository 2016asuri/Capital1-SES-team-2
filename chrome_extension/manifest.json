{
  "name": "ROUNDED",
  "description": "Banking for good.",
	"version": "1",
	"manifest_version": 2,
   "web_accessible_resources": ["coins.png"],
  "permissions": [
    "activeTab",
    "webNavigation",
    "*://*/*"
  ],
  "content_scripts": [{
     "matches": ["https://www.amazon.com/gp/cart/view.html?ref=nav_cart"],
     "js": ["background.js"]
 }],
  "browser_action": {
    "default_title": "ROUNDED",
    "default_icon": "icon.png"
  }
}
